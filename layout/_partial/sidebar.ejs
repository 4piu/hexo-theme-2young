<% if (position === "left") { %>
    <aside id="left-sidebar" class="sidebar">
        <% if (theme.author.enabled && is_home()) { %>
            <%- partial('_widget/profile') %>
        <% } %>
        <% if (theme.toc.enabled && is_post()) { %>
            <div id="toc">
                <%- toc(page.content) %>
            </div>
        <% } %>
    </aside>
<% } else if (position === "right") { %>
    <aside id="right-sidebar" class="sidebar">

    </aside>
<% } %>